<h1>GET Request Received</h1>
<h3>By: John Fitzpatrick</h3>

<ul>
  {{#each found}}
    <li>{{this.name}}: {{this.value}}
  {{/each}}
</ul>
<!-- GET formatting below -->
{{#if found}}
<table border="1px">
  <caption><b>Query String Parameters Sent</b></caption>
  <thead>
    <tr>
      <th>Key</th>
      <th>Value</th>
    </tr>
  </thead>

  <tbody>
    {{#each found}}
    <tr>
      <td>{{this.name}}</td>
      <td>{{this.value}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
{{/if}}
